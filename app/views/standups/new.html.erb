<h1>New Standup</h1>

<%= form_for @standup, url: standups_path do |f| %>
  <p>
    <%= f.label :yesterday %><br>
    <%= f.text_area :yesterday %>
  </p>
 
  <p>
    <%= f.label :today %><br>
    <%= f.text_area :today %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>

<%= link_to 'Back', standups_path %>