$("ul.errors").html("")
<% if @standup.errors.any? %>
  <% @standup.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
  <% end %>
<% else %>
  $(".standup-index").html("<%= escape_javascript(render 'index') %>")
  $("#standup-modal").modal("hide")
<% end %>