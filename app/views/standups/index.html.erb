<h1>Standups: </h1>

<% @standups.each do |standup| %>
	
	<% standup.timestamps.created_on.to_date %>
	<h2>Yesterday items:</h2>
	<% standup.yesterday_items.each do |yesterday_item| %>
		<p>
			<strong>Number:</strong>
			<%= yesterday_item.number %>
		</p>
 
		<p>
			<strong>Item:</strong>
			<%= yesterday_item.item %>
		</p>
	<% end %>
<% end %>


<%= link_to 'New Standup', new_standup_path %>


<%= link_to 'Home', controller: 'welcome' %>